<?xml version="1.0"?>

<PropertyList>
 <comment>
  <![CDATA[
	Usage: [Commands] --generic=socket,in,0.5,127.0.0.1,9878,tcp,fg_write_control
  ]]>
 </comment>
 <generic>
  <input>
   <line_separator>\r\n</line_separator>
   <var_separator>,</var_separator>

   <!-- Flight Controls -->
   <chunk>
    <name>aileron</name>
	<type>float</type>
	<node>/controls/flight/aileron</node>
   </chunk>

   <chunk>
	<name>aileron-trim</name>
	<type>float</type>
	<node>/controls/flight/aileron-trim</node>
   </chunk>

   <chunk>
	<name>elevator</name>
	<type>float</type>
	<node>/controls/flight/elevator</node>
   </chunk>

   <chunk>
	<name>elevator-trim</name>
	<type>float</type>
	<node>/controls/flight/elevator-trim</node>
   </chunk>

   <chunk>
	<name>rudder</name>
	<type>float</type>
	<node>/controls/flight/rudder</node>
   </chunk>

   <chunk>
	<name>rudder-trim</name>
	<type>float</type>
	<node>/controls/flight/rudder-trim</node>
   </chunk>

   <chunk>
	<name>flaps</name>
	<type>float</type>
	<node>/controls/flight/flaps</node>
   </chunk>
  </input>
 </generic>
</PropertyList>
